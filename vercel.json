{
    "version": 2,
    "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node",
          "config": {
            "outputs": {
              "main": ".output/index.js"
            }
          }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
      ]
}